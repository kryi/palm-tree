<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="vo">
 <class name="Card" table="c_card">
  <id name="id">
   <generator class="native"/>
  </id>
  <property column="name" generated="never" lazy="false" name="name"/>
  <property column="sex" generated="never" lazy="false" name="sex"/>
  <property column="phone" generated="never" lazy="false" name="phone"/>
  <property column="email" generated="never" lazy="false" name="email"/>
  <property column="address" generated="never" lazy="false" name="address"/>
  <property column="userId" generated="never" lazy="false" name="userId"/>
 </class>
</hibernate-mapping>
